<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/back"
    tools:context="com.example.elashry.aseer.Activities.Home"
    >
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/fff"
        android:layout_alignBottom="@+id/circle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="873dp" />

    <com.szugyi.circlemenu.view.CircleLayout
        android:id="@+id/circle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        app:isRotating="true"
        app:radius="200dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="130dp"
        android:layout_marginBottom="130dp">

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/apsent"
            android:src="@drawable/eicon10"
            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/homework"
            android:src="@drawable/icon2"
            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/activity"
            android:src="@drawable/eicon3"

            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/news"
            android:src="@drawable/eicon4"

            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/table"
            android:src="@drawable/eicon5"

            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/arrive"
            android:src="@drawable/eicon6"
            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/sharf"
            android:src="@drawable/eicon7"

            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/rsoom"
            android:src="@drawable/eicon8"

            />

        <com.szugyi.circlemenu.view.CircleImageView
            style="@style/icone_la"
            android:id="@+id/state"
            android:src="@drawable/eicon9"

            />

    </com.szugyi.circlemenu.view.CircleLayout>

    <Button
        android:layout_height="wrap_content"
        android:id="@+id/btn_out"
        android:backgroundTint="#1b7c75"
        android:layout_width="200dp"
        android:textColor="#f5fafa"
        android:textStyle="bold"
        android:textSize="25dp"
        android:text="@string/out"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="15dp"/>
    <!--<ImageView-->
    <!--android:layout_width="45dp"-->
    <!--android:layout_height="45dp"-->
    <!--app:srcCompat="@drawable/mss"-->
    <!--android:layout_marginLeft="180dp"-->
    <!--android:layout_marginTop="130dp"-->
    <!--android:id="@+id/imageView"-->
    <!--/>-->


</RelativeLayout>
